{
  "name": "attribix-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf build .vite node_modules/.vite .shopify/dev-bundle",
    "clean:shopify": "rimraf .shopify/dev-bundle",
    "clean:vite": "rimraf .vite node_modules/.vite",
    "build": "rimraf build && remix vite:build",
    "predev:safe": "node ./scripts/predev.mjs",
    "dev": "npm run predev:safe && shopify app dev",
    "dev:fly": "npm run predev:safe && shopify app dev --tunnel-url https://attribix-app.fly.dev:443",
    "dev:fly:reset": "npm run predev:safe && shopify app dev --reset --tunnel-url https://attribix-app.fly.dev:443",
    "dev:remix": "remix vite:dev",
    "dev:remix:force": "remix vite:dev --force",
    "start": "remix-serve ./build/server/index.js --host :: --port 3000",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@remix-run/dev": "^2.17.2",
    "@remix-run/node": "^2.17.2",
    "@remix-run/react": "^2.17.2",
    "@remix-run/serve": "2.17.2",
    "@shopify/app-bridge": "^3.7.5",
    "@shopify/app-bridge-react": "4.2.7",
    "@shopify/polaris": "13.9.5",
    "@shopify/shopify-api": "12.1.1",
    "@shopify/shopify-app-remix": "4.0.5",
    "@shopify/shopify-app-session-storage": "4.0.2",
    "@shopify/shopify-app-session-storage-memory": "5.0.2",
    "prisma": "^6.19.0",
    "recharts": "2.15.4",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.21",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
