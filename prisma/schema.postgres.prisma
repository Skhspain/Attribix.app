model Session {
  id               String   @id
  shop             String
  state            String?
  isOnline         Boolean
  scope            String?
  accessToken      String?
  expires          DateTime?
  onlineAccessInfo Json?
  userId           BigInt?
  content          Json?
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt

  @@index([shop])
}
