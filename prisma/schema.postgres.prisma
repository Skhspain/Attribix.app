datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")   // PgBouncer on 5432 (runtime)
  directUrl = env("DIRECT_URL")     // Direct Postgres on 5433 (migrations/db push)
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}
